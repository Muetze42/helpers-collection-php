{
    "name": "norman-huth/helpers-collection-php",
    "license": "MIT",
    "support": {
        "source": "https://github.com/Muetze42/helpers-collection-laravel",
        "docs": "https://docs.huth.it/helpers-collection"
    },
	"authors": [
        {
            "name": "Norman Huth",
            "homepage": "https://huth.it"
        }
    ],
    "autoload": {
        "psr-4": {
            "NormanHuth\\Helpers\\": "src/"
        },
        "files": [
            "functions/helpers.php"
        ]
    },
    "require": {
        "php": "^8.1",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "league/commonmark": "^2.2",
        "nesbot/carbon": "^2.53.1",
        "norman-huth/markdown": "^v1.0.2",
        "ramsey/uuid": "^3.0|^4.0",
        "symfony/uid": "^5.1|^6.0",
        "symfony/var-dumper": "^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0.0|^9.0.0|^10.0.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
